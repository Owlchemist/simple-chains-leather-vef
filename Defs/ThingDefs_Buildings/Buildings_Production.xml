<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ThingDef ParentName="BuildingBase">
		<defName>Owl_TanningVat</defName>
		<label>tanning vat</label>
		<description>Tanning liquors can be poured here, and raw hides and pelts can then be soaked in it for a couple days to yield tanned leather.</description>    
		<thingClass>ItemProcessor.Building_ItemProcessor</thingClass>
		<graphicData>
			<texPath>Things/Building/Production/tanning_vat</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			
			<drawSize>(1.5,1.5)</drawSize>
			<damageData>
				<rectN>(0.125,0.296875,0.734375,0.421875)</rectN>
				<rectS>(0.125,0.296875,0.734375,0.421875)</rectS>
				<rectE>(0.234375,0.296875,0.5,0.546875)</rectE>
				<rectW>(0.234375,0.296875,0.5,0.546875)</rectW>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
			<shadowData>
				<volume>(0.8, 0.8, 0.8)</volume>
				<offset>(0,0,-0.25)</offset>
			</shadowData>
		</graphicData>
		<rotatable>false</rotatable>
		<uiIconPath>Things/Building/Production/tanning_vat</uiIconPath>
		<stuffCategories>
			<li>Metallic</li>
			<li>Woody</li>
		</stuffCategories>
		<costStuffCount>20</costStuffCount>
		<altitudeLayer>Building</altitudeLayer>
		<fillPercent>0.6</fillPercent>
		<useHitPoints>True</useHitPoints>
		<statBases>
			<Mass>5</Mass>
			<MaxHitPoints>90</MaxHitPoints>
			<WorkToBuild>1000</WorkToBuild>
			<Flammability>1.0</Flammability>
		</statBases>
		<designationCategory>Production</designationCategory>
		<passability>PassThroughOnly</passability>
		<comps>
			<li Class="ItemProcessor.CompProperties_ItemProcessor">
				<buildingOnGraphic>Things/Building/Production/tanning_vat_full</buildingOnGraphic>
				<numberOfInputs>1</numberOfInputs>
				<showProgressBar>true</showProgressBar>
				<acceptsNoneAsInput>false</acceptsNoneAsInput>
				<InsertFirstItemDesc>IP_ChooseOutput</InsertFirstItemDesc>
				<isSemiAutomaticMachine>true</isSemiAutomaticMachine>
				<isMachineSpecifiesOutput>true</isMachineSpecifiesOutput>
				<chooseIngredientsIcon>Things/Item/Resource/PreparedHide</chooseIngredientsIcon>
				<bringIngredientsIcon>Things/Item/Resource/PreparedHide</bringIngredientsIcon >
				<resetSemiautomaticIcon>Things/Item/Resource/PreparedHide</resetSemiautomaticIcon>
				<noPowerDestroysProgress>false</noPowerDestroysProgress>
				<isTemperatureDependingMachine>true</isTemperatureDependingMachine>
				<minTemp>0</minTemp>
				<maxTemp>99</maxTemp>
				<destroyIngredientsAtStartOfProcess>true</destroyIngredientsAtStartOfProcess>
			</li>
			<li Class="CompProperties_Forbiddable"/>
		</comps>
		<tickerType>Rare</tickerType>
		<minifiedDef>MinifiedThing</minifiedDef>
		<thingCategories>
			<li>BuildingsProduction</li>
		</thingCategories>
	</ThingDef>
	
	<ItemProcessor.CombinationDef>
		<defName>Owl_ProcessLeather_Plain</defName>
		<building>Owl_TanningVat</building>
		<items>
			<li>Hide_Plain</li>
		</items>
		<secondItems>
			<li>Owl_TanningLiquor</li>
		</secondItems>
		<amount>
			<li>75</li>
			<li>25</li>
		</amount>
		<result>Leather_Plain</result>
		<yield>75</yield>
		<singleTimeIfNotQualityIncreasing>1.8</singleTimeIfNotQualityIncreasing>
	</ItemProcessor.CombinationDef>
	
